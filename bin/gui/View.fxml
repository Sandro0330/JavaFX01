<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <ComboBox fx:id="comboBoxPessoa" layoutX="22.0" layoutY="33.0" onAction="#onComboBoxPessoaAction" prefWidth="150.0" promptText="Selecione uma Pessoa..." />
      <Button fx:id="btnTudo" layoutX="200.0" layoutY="33.0" mnemonicParsing="false" onAction="#onBtnTudoAction" text="Tudo" />
   </children>
</AnchorPane>
